{
  "name": "x-env-es",
  "version": "1.0.1",
  "description": "Project (.env) environment manager",
  "main": "./index.js",
  "module": "./cjs",
  "types": "/types",
  "typings": "/types",
  "esm": {
    "mode": "auto"
  },
  "scripts": {
    "example:env:dev": "rimraf ./.env && node -r dotenv/config xenvExample dotenv_config_path=./xenvExample/envs/dev.env",
    "example:env:prod": "rimraf ./.env && node -r dotenv/config xenvExample dotenv_config_path=./xenvExample/envs/prod.env",
    "example:env:test": "rimraf ./.env && node -r dotenv/config xenvExample dotenv_config_path=./xenvExample/envs/test.env"
  },
  "keywords": [
    ".env",
    "env",
    "dotenv",
    "x-env",
    "dev.env",
    "prod.env",
    "test.env",
    "process.env"
  ],
  "engines": {
    "node": "*",
    "npm": "*"
  },
  "author": "anon",
  "license": "MIT",
  "dependencies": {
    "read-env-file": "^2.0.3-0",
    "dotenv": "^10.0.0",
    "dotenv-expand": "^5.1.0",
    "x-utils-es": "^2.4.0"
  }
}
