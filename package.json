{
  "name": "x-env-es",
  "version": "1.1.0",
  "description": "Project (.env) environment manager",
  "main": "./index.js",
  "module": "./cjs",
  "types": "/types",
  "typings": "/types",
  "esm": {
    "mode": "auto"
  },
  "scripts": {
    "app:dev": "rimraf ./.env && node -r app.example dotenv_config_path=./app.example/XENV/dev.env",
    "app:prod": "rimraf ./.env && node -r app.example dotenv_config_path=./app.example/XENV/prod.env",
    "app:test": "rimraf ./.env && node -r app.example dotenv_config_path=./app.example/XENV/test.env"
  },
  "keywords": [
    ".env",
    "env",
    "dotenv",
    "x-env",
    "dev.env",
    "prod.env",
    "test.env",
    "process.env"
  ],

  "engines": {
    "node": "*",
    "npm": "*"
  },

  "repository": {
    "type": "git",
    "url": "https://github.com/anonym101/x-env"
  },
  "homepage": "https://github.com/anonym101/",

  "author": "anon",
  "license": "MIT",
  "dependencies": {
    "read-env-file": "^2.0.3-0",
    "dotenv": "^10.0.0",
    "dotenv-expand": "^5.1.0",
    "x-utils-es": "^2.4.0"
  }
}
