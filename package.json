{
  "name": "x-env-es",
  "version": "1.1.1",
  "description": "Project (.env) environment manager",
  "main": "./index.js",
  "module": "./cjs",
  "types": "/types",
  "typings": "/types",
  "esm": {
    "mode": "auto"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/anonym101/x-env"
  },
  "homepage": "https://github.com/anonym101/",
  "scripts": {
    "app:dev": "node ./app.example xenv_path=./app.example/XENV/dev.env",
    "app:prod": "node ./app.example xenv_path=./app.example/XENV/prod.env",
    "app:test": "node ./app.example xenv_path=./app.example/XENV/test.env",
    "dev:example": "node -r ./DEV ./app.example/cli xenv_debug=1 xenv_dir=./inval",
    "prod:example": "node -r ./PROD ./app.example/cli xenv_debug=1",
    "test:example": "node -r ./TEST ./app.example/cli xenv_debug=1",
    "example":"node ./example_script.js",
    "cross:example":"cross-env-shell GREETING=Hi NAME=Joe node ./example_script.js",
    "build": "rimraf dist/ &&tsc --project .",
    "build:cjs": " npm run build&& babel --config-file ./cjs/.babelrc ./dist --out-dir ./cjs",
    "lint:install": "npm i tslint --save-dev",
    "lint:fix": "./node_modules/.bin/tslint  --fix -c tslint.json src/**/*",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    ".env",
    "env",
    "dotenv",
    "x-env-es",
    "dev.env",
    "prod.env",
    "test.env",
    "process.env"
  ],
  "engineStrict": true,
  "engines": {
    "node": "~12.14.0",
    "npm": "~6.13.4"
  },
  "author": "anon",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@types/node": "^16.10.9",
    "babel-eslint": "^10.1.0",
    "babel-preset-minify": "^0.5.1",
    "cross-env": "^7.0.3",
    "rimraf": "^3.0.2",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "tslint": "^6.1.3",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "dotenv": "^10.0.0",
    "dotenv-expand": "^5.1.0",
    "read-env-file": "^2.0.3-0",
    "x-utils-es": "^2.4.0"
  }
}
